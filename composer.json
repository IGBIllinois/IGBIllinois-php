    "keywords": ["template", "composer", "package"],
    "license": "GPL-3.0-only",
    "homepage": "https://www.github.com/IGBIllinois/IGBIllinois-php",
    "authors": [
        {
            "name": "David Slater",
            "email": "dslater@illinois.edu"
        }
    ],
    "type": "project",
    "require": {
